<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://tfs:8080/common</TeamFoundationServer><TeamProject>Project1</TeamProject><Wiql>SELECT [System.Id], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State], [System.Tags] FROM WorkItems WHERE [System.TeamProject] = @project  AND  [System.WorkItemType] IN GROUP 'Категория ответа на проверку кода'  AND  [System.AssignedTo] = @me  AND  [System.State] = 'Запрашивается' ORDER BY [System.Id] </Wiql></WorkItemQuery>